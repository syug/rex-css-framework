{
  "name": "rex-css-framework",
  "version": "0.2.2",
  "description": "Rakuten Experience Project, UX/UI Design CoE Section",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/rakuten-frontend/rex-css-framework"
  },
  "scripts": {
    "start": "NODE_ENV=development ENV_TYPE=development webpack-dev-server --content-base dev --port 9090 --host 0.0.0.0 --public localhost:9090 --config webpack.config.dev.js",
    "server": "node server.js",
    "flow": "flow || true",
    "build": "webpack --config webpack.prod.js --color -p --progress",
    "postbuild": "cp -r ./src/style/fonts ./build/ && cp -r ./build/css-framework/themes/ ./build/themes/",
    "build-font": "svgo ./icons/ && iconfont -f icons --scss-dest ./src/style/core/ --fonts-dest ./src/style/fonts/ --scss-name _glyphicons --scss-fonts-url ../fonts -fn rex-icon",
    "build-font-preview": "svgo ./icons/ && iconfont -f icons -sn rex --html true -fn rex-icon",
    "eslint": "eslint --max-warnings 0 ./src",
    "eslint-fix": "eslint ./src --fix"
  },
  "author": "Josh Worley, Claudio Moya, Alexander King, Linmic, Shugo Sasito, Akinori Noguchi",
  "license": "MIT",
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9",
    "ios >= 8",
    "android >= 4.1"
  ],
  "devDependencies": {
    "autoprefixer": "^8.1.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.2",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.7",
    "empty": "^0.10.1",
    "eslint": "^3.19.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.1",
    "flow-bin": "^0.39.0",
    "html-webpack-plugin": "^2.30.1",
    "lodash": "^4.17.4",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.1.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.17.0",
    "url-loader": "^0.6.2",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.4"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.5",
    "clean-webpack-plugin": "^0.1.17",
    "immutable": "^3.8.1",
    "js-beautify": "^1.6.14",
    "js-logger": "^1.3.0",
    "page": "^1.7.1",
    "pre-commit": "^1.2.2",
    "prismjs": "^1.6.0",
    "prop-types": "^15.5.10",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "redux": "^3.6.0",
    "uglifyjs-webpack-plugin": "^1.1.2",
    "webpack-merge": "^4.1.1"
  }
}
